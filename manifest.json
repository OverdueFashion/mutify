{
    "manifest_version": 2,
  
    "name": "mutify",
    "description": "Ad free spotify",
    "version": "1.0",
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "permissions": ["http://open.spotify.com/*"],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }